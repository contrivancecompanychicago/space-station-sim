// Place your settings in this file to overwrite default and user settings.
{
      "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/coverage/**/*.*": true,
    "**/dist/**/*.*": true,
    "**/flow-typed/**/*.*": true,
    "**/.vscode/chrome/**/*.*": true,
    "**/.vscode/chrome/**/*": true
  },
    "javascript.validate.enable": false,
    "vsicons.presets.angular": false,
    "lcov.path": "coverage/lcov.info",
    "lcov.watch": [{
        "pattern": "src/**/*.js",
        "command": "npm run test"
    }]
}